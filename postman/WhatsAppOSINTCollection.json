		{
			"name": "GET Privacy",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", () => pm.response.to.have.status(200));\r",
							"pm.test(\"Response has privacy data\", () => {\r",
							"    const jsonData = pm.response.json();\r",
							"    pm.expect(jsonData).to.be.an('object');\r",
							"    pm.expect(jsonData).to.have.property('phone');\r",
							"    const expectedPhone = pm.environment.get(\"phone\");\r",
							"    console.log(\"Environment phone value:\", expectedPhone); \r",
							"    if (expectedPhone) {\r",
							"        pm.expect(jsonData.phone).to.equal(expectedPhone);\r",
							"    } else {\r",
							"        pm.expect(jsonData.phone).to.equal(\"13022612667\");\r",
							"    }\r",
							"    pm.expect(Object.keys(jsonData).length).to.be.at.least(2);\r",
							"    const additionalProperty = Object.keys(jsonData).find(key => key !== 'phone');\r",
							"    pm.expect(jsonData[additionalProperty]).to.be.a('string');\r",
							"});\r",
							"pm.test(\"Response time is less than 1000ms\", () => {\r",
							"    pm.expect(pm.response.responseTime).to.be.below(1000);\r",
							"});"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
